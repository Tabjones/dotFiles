[user]
    name = Federico Spinelli
    email = fspinelli@gmail.com
[alias]
    st = status
    ci = commit -v
    co = checkout
    di = diff
    amend = commit --amend
    aa = add --all
    fa = fetch --all
    pom = push origin master
    b = branch
    ru = rebase -i @{u}
    mff = merge --no-ff
    ignored = !git ls-files -v | grep "^[[:lower:]]"
[push]
    default = simple
[color]
    ui = always
[help]
    autocorrect = 1
[pull]
    rebase = true
